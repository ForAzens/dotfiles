urxvt.foreground: White
urxvt.saveLines: 20000
urxvt.scrollBar: off
!urxvt.font: xft:Hack:size=10:autohint=true:antialias=true
URxvt*font: xft:Iosevka:pixelsize=20:minspace=False, xft:DejaVu Sans:pixelsize=18
URxvt*boldFont: xft:Iosevka:pixelsize=20:minspace=False, xft:DejaVu Sans:pixelsize=18
URxvt*letterSpace: 0


